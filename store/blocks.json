{
  "store.checkout.cart": {
    "blocks": ["checkout-cart"]
  },
  "store.checkout.cart-add": {
    "blocks": ["checkout-cart-add"]
  },
  "store.checkout.container": {
    "children": ["responsive-layout.desktop#checkout"]
  },
  "store.checkout.identification": {
    "children": ["flex-layout.row#identification"]
  },

  "flex-layout.row#identification": {
    "children": ["checkout-identification"],
    "props": {
      "blockClass": "identificationWrapper",
      "preserveLayoutOnMobile": true,
      "width": "100%",
      "fullWidth": true
    }
  },

  "responsive-layout.desktop#checkout": {
    "children": ["flex-layout.row#container-desktop"]
  }
}
