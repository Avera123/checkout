{
  "store.checkout": {
    "allowConditions": true,
    "component": "*",
    "before": ["header.full"],
    "after": ["footer", "pixels"],
    "preview": {
      "type": "transparent",
      "height": 600
    }
  },
  "store.checkout.cart": {
    "around": ["homeWrapper"],
    "allowed": ["checkout-cart"],
    "preview": {
      "type": "box",
      "height": 1400
    }
  },
  "store.checkout.cart-add": {
    "around": ["homeWrapper"],
    "allowed": ["checkout-cart-add"]
  },
  "store.checkout.container": {
    "before": ["header.full.checkout"],
    "around": ["checkout-container"],
    "after": ["footer.checkout"],
    "composition": "children",
    "render": "client"
  },
  "store.checkout.identification": {
    "before": ["header.full.checkout"],
    "around": ["checkout-container"],
    "after": ["footer.checkout"],
    "composition": "children",
    "render": "client"
  },
  "header.full.checkout": {
    "component": "Empty"
  },
  "footer.checkout": {
    "component": "Empty"
  },
  "place-order": {
    "component": "PlaceOrder"
  },
  "checkout-product-list": {
    "component": "ProductListWrapper",
    "allowed": ["product-list"]
  },
  "product-counter": {
    "component": "ProductCounter"
  },
  "back-to-cart-button": {
    "component": "BackToCartButton"
  },
  "summary-wrapper": {
    "component": "SummaryWrapper",
    "allowed": ["checkout-summary"]
  }
}
