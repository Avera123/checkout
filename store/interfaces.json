{
  "store.checkout": {
    "allowConditions": true,
    "component": "*",
    "before": ["header.full"],
    "after": ["footer", "pixels"],
    "preview": {
      "type": "transparent",
      "height": 600
    }
  },
  "store.checkout.cart": {
    "around": ["homeWrapper"],
    "allowed": ["checkout-cart"],
    "preview": {
      "type": "box",
      "height": 1400
    }
  },
  "store.checkout.cart-add": {
    "around": ["homeWrapper"],
    "allowed": ["checkout-cart-add"]
  },
  "store.checkout.container": {
    "before": ["header.full.checkout"],
    "around": ["checkout-container"],
    "after": ["footer.checkout"],
    "composition": "children",
    "render": "client"
  },
  "header.full.checkout": {
    "component": "Empty"
  },
  "footer.checkout": {
    "component": "Empty"
  }
}
